# RunPod Configuration
# For local testing and deployment only. Do not commit secrets.

# RunPod API key (needed for deployment scripts)
RUNPOD_API_KEY=your_runpod_api_key_here

# Hugging Face token (for local testing only)
# NOTE: For production deployment, add this as a RunPod secret with key "HUGGING_FACE_TOKEN" 
# in the RunPod dashboard when creating a template.
HUGGINGFACE_TOKEN=your_huggingface_token_here

# Runpod Network Volume (optional)
# NETWORK_VOLUME_ID=your_network_volume_id

# Model Configuration
MODEL_ID=nari-labs/Dia-1.6B
COMPUTE_DTYPE=float16

# Generation Parameters
DEFAULT_TEMPERATURE=1.3
DEFAULT_TOP_P=0.95
# DEFAULT_SEED=42  # Uncomment to set a default seed for reproducible outputs

# System Configuration
GPU_TYPES="NVIDIA A4000,NVIDIA RTX 4000,NVIDIA RTX 3090"
MIN_WORKERS=0
MAX_WORKERS=3
IDLE_TIMEOUT=300
FLASH_BOOT=true
CONTAINER_DISK_SIZE=20
EXECUTION_TIMEOUT=600